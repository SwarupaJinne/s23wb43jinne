extends layout
block content
  h1= title
  p Welcome 
  p Be with the one with whom you wanna be.
  button(type="button", class="btn btn-info" onclick="doReduce()") Press Me
  div#reduce Reduce the value
  script.
    let count = 20;
    let change = -1;
    function doReduce() {
      count += change;
      change -= 1;
      document.getElementById('reduce').textContent = `Count ${count} with change ${change}`;
    }
 
  a(href="mango") Search Results


